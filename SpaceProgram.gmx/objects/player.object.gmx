                     {
                      sprite_index=stayleftdown;break;
                     }
            }
        break;
        }
    case d_Up:
         {
          if y -10 &gt; 0 {dy=-10};sprite_index=walkup;oldlad=lad;break;
         }
    case d_Down:
         {
          if y +10 &lt; room_height {dy=10};sprite_index=walkdown;oldlad=lad;break;     

         }
    case d_DownRight:
         {
          if y+sq&lt;room_height&amp;&amp; x+sq&lt;room_width {dy=sq;dx=sq};sprite_index=walkrightdown;oldlad=lad;break;
         }
    case d_DownLeft:
         {
          if y+sq&lt;room_height&amp;&amp;  x-sq&gt;0 {dy=sq;dx=-sq};sprite_index=walkleftdown;oldlad=lad;break;
         }
